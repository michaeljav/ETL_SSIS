      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{2C326D0A-C7C5-4899-A67D-37AD2E8EBFB3}"
          SQLTask:SqlStatementSource="delete ContratosContraloria&#xA;&#xA;declare @UC table (CodigoUnidadCompra varchar(10))&#xA;&#xA;insert into @UC&#xA;select distinct CodigoUnidadCompra &#xA;from ContratosPortalTransaccional &#xA;&#xA;insert into ContratosContraloria&#xA;select&#x9;CodigoUnidadCompra, &#xA;&#x9;&#x9;UnidadCompra, &#xA;&#x9;&#x9;CodigoProceso, &#xA;&#x9;&#x9;CodigoContrato, &#xA;&#x9;&#x9;RPE as CodigoProveedor, &#xA;&#x9;&#x9;RPE, &#xA;&#x9;&#x9;RazonSocial, &#xA;&#x9;&#x9;TipoDocumento, &#xA;&#x9;&#x9;NumeroDocumento, &#xA;&#x9;&#x9;EsMiPyme, &#xA;&#x9;&#x9;Genero, &#xA;&#x9;&#x9;[URL], &#xA;&#x9;&#x9;ObjetoDelContrato, &#xA;&#x9;&#x9;SubObjetoDelContrato, &#xA;&#x9;&#x9;Modalidad,&#xA;&#x9;&#x9;EstadoProceso, &#xA;&#x9;&#x9;EstadoContrato, &#xA;&#x9;&#x9;FechaAprobacion, &#xA;&#x9;&#x9;FechaInicioContrato, &#xA;&#x9;&#x9;FechaFinContrato, &#xA;&#x9;&#x9;Moneda, &#xA;&#x9;&#x9;ValorContratado, &#xA;&#x9;&#x9;MetodoPago,&#xA;&#x9;&#x9;'PT' as Origen&#xA;from ContratosPortalTransaccional&#xA;where FechaAprobacion &gt;= '2016-01-01'&#xA;&#xA;union all&#xA;&#xA;select&#x9;Cod_Unidad_Compras as CodigoUnidadCompra,&#xA;&#x9;&#x9;UnidadCompra,&#xA;&#x9;&#x9;IdentificacionTramite as CodigoProceso,&#xA;&#x9;&#x9;IdentificacionDeContrato as CodigoContrato,&#xA;&#x9;&#x9;RPE as CodigoProveedor, &#xA;&#x9;&#x9;RPE,&#xA;&#x9;&#x9;Proveedor as RazonSocial,&#xA;&#x9;&#x9;TipoDocumento,&#xA;&#x9;&#x9;Num_Documento as NumeroDocumento,&#xA;&#x9;&#x9;case&#xA;&#x9;&#x9;&#x9;when EsMiPyme = 'No' then 0&#xA;&#x9;&#x9; else 1 &#xA;&#x9;&#x9;end as EsMiPyme,&#xA;&#x9;&#x9;case &#xA;&#x9;&#x9;&#x9;when GeneroUnificado = 'Masculino' then 'M' &#xA;&#x9;&#x9;  else 'F'&#x9;&#xA;&#x9;&#x9;end as Genero,&#xA;&#x9;&#x9;null as [URL],&#xA;&#x9;&#x9;null as ObjetoDelContrato, &#xA;&#x9;&#x9;null as SubObjetoDelContrato, &#xA;&#x9;&#x9;ModalidadCompra as Modalidad,&#xA;&#x9;&#x9;'Aprobado' as EstadoProceso,  &#xA;&#x9;&#x9;'Aprobado' as EstadoContrato,&#xA;&#x9;&#x9;fch_aprobacion as FechaAprobacion, &#xA;&#x9;&#x9;fch_aprobacion as FechaInicioContrato, &#xA;&#x9;&#x9;null as FechaFinContrato,&#xA;&#x9;&#x9;'DOP' as Moneda,&#xA;&#x9;&#x9;ValorTotal as ValorContratado,&#xA;&#x9;&#x9;null as MetodoPago,&#xA;&#x9;&#x9;'SIGEF' as Origen&#xA;from SIGEF.dbo.ContratosConCertificacionesMIC&#xA;where Cod_Unidad_Compras not in (select CodigoUnidadCompra from @UC)&#xA;  and fch_aprobacion &gt;= '2016-01-01'" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
