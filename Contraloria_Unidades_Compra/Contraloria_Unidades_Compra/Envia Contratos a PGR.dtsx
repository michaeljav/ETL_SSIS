	set @back = -3
end

SELECT 

CodigoUnidadCompra,UnidadCompra,CodigoProceso,CodigoContrato,CodigoProveedor,RPE,RazonSocial,TipoDocumento,NumeroDocumento,EsMiPyme,Genero,URL,ObjetoDelContrato,SubObjetoDelContrato,Modalidad,EstadoProceso,EstadoContrato,FechaAprobacion,FechaInicioContrato, FechaFinContrato,Moneda,ValorContratado,MetodoPago,Origen

