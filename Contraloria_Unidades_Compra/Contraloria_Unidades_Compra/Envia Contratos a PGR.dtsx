                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">declare @weekday tinyint = datepart(WEEKDAY,getdate()) , @back smallint = -1

declare @today date = null,
		@today7am datetime2 =  null,
		@todayNoon datetime2 =  null

set @today = GETDATE()
set @today7am = dateadd(hour,11,cast(@today as datetime2))
set @todayNoon = dateadd(hour,16,cast(@today as datetime2))

if @weekday = 2
begin
	set @back = -3
end

SELECT 

		CodigoUnidadCompra,
		UnidadCompra,
		CodigoProceso,
		CodigoContrato,
		CodigoProveedor,
		RPE,
		RazonSocial,
		TipoDocumento,
		NumeroDocumento,
		EsMiPyme,
		Genero,
		URL,
		ObjetoDelContrato,
		SubObjetoDelContrato,
		Modalidad,
		EstadoProceso,
		EstadoContrato,
		FechaAprobacion,
		FechaInicioContrato,
		FechaFinContrato,
		Moneda,
		ValorContratado,
		MetodoPago,Origen

FROM ContratosContraloria

where FechaAprobacion &gt; @today7am and FechaAprobacion &lt;= @todayNoon</property>
